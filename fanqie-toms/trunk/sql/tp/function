DECLARE
	r RECORD ; del bool ; num int4 := 0 ; SQL "varchar" ; typeId int4 = -1 ; status "varchar" :='3'; ota_id "varchar" :='101';
BEGIN
  SQL:='SELECT  b.ota_wg_id,b.id,b.inn_id,b.sj FROM bang_inn b
INNER JOIN ota_inn_ota oio ON oio.id = b.ota_wg_id
where b.company_id=''d0392bc8-131c-8989-846e-c81c66011111''';

FOR r IN EXECUTE SQL LOOP

UPDATE  ota_inn_ota  set bang_inn_id = r.id ,inn_id=r.inn_id,ota_info_id='1',sj=r.sj where id=r.ota_wg_id;


END LOOP ;


END;