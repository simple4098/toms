<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fanqielaile.toms.dao.DailyInfosDao">
    <resultMap id="BaseResultsMap" type="com.fanqielaile.toms.model.DailyInfos">
        <id column="id" property="id"/>
        <result column="day" property="day"/>
        <result column="price" property="price"/>
        <result column="created_date" property="createdDate"/>
        <result column="creator_id" property="creatorId"/>
        <result column="order_id" property="orderId"/>
    </resultMap>
    <sql id="Base_Column_List">
        id,day,price,created_date,creator_id,order_id
    </sql>
    <insert id="insertDailyInfos" parameterType="com.fanqielaile.toms.model.DailyInfos">
        INSERT INTO ota_daily_infos(id,day,price,created_date,creator_id,order_id)
        VALUES
        (#{uuid},#{day},#{price},#{createdDate},#{creatorId},#{orderId})
    </insert>
</mapper>